{
  "name": "quickbid",
  "private": true,
  "version": "1.0.0",
  "license": "Proprietary",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --config vite.config.production.ts",
    "build:analyze": "vite build --config vite.config.production.ts && npx vite-bundle-analyzer dist",
    "typecheck": "tsc -p tsconfig.strict.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "seed:users": "node scripts/seed-users.mjs",
    "seed:products": "node scripts/seed-products.mjs",
    "seed:seller": "node scripts/seed-seller-dashboard.mjs",
    "preview": "vite preview",
    "preview:production": "vite preview --config vite.config.production.ts --port 3022",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "playwright test tests/e2e/",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "node tests/performance/lighthouse.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:production": "playwright test --config=playwright.config.production.ts tests/e2e/",
    "test:ux": "playwright test tests/e2e/ --grep=\"UX|ux\"",
    "test:visual": "playwright test tests/e2e/ --grep=\"visual|design\"",
    "test:forms": "playwright test tests/e2e/forms.spec.ts",
    "test:accessibility": "playwright test tests/e2e/accessibility.spec.ts",
    "test:dashboard": "playwright test tests/e2e/dashboard-ux.spec.ts",
    "test:buyer-flow": "playwright test tests/e2e/buyer-flow.spec.ts",
    "test:seller-flow": "playwright test tests/e2e/seller-flow.spec.ts",
    "test:product-detail": "playwright test tests/e2e/product-detail-ux.spec.ts",
    "test:real-products": "playwright test tests/e2e/real-products.spec.ts",
    "test:beta-users": "playwright test tests/e2e/beta-user-access.spec.ts",
    "test:beta-feedback": "playwright test tests/e2e/beta-feedback-loop.spec.ts",
    "test:modern-ui": "playwright test tests/e2e/modern-ui.spec.ts",
    "test:demo-login": "playwright test tests/e2e/demo-login-validation.spec.ts",
    "test:ux-system": "playwright test tests/e2e/ux-system-validation.spec.ts",
    "test:component": "vitest run tests/components/*.test.tsx",
    "test": "vitest",
    "live-backend": "node --loader ts-node/esm backend/server.ts",
    "worker:events": "node --loader ts-node/esm backend/workers/eventsEnrichmentWorker.ts",
    "worker:rules": "node --loader ts-node/esm backend/workers/ruleEngineWorker.ts",
    "worker:notify": "node --loader ts-node/esm backend/workers/notificationWorker.ts",
    "deploy": "./scripts/deploy.sh"
  },
